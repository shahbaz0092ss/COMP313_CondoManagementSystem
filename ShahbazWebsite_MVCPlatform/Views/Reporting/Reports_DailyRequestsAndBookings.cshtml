@model IEnumerable<ShahbazWebsite_MVCPlatform.Models.Booking>

    @{
    ViewData["Title"] = "Reports - Daily Requests And Bookings";
    }



    <div style="color:rgb(0, 102, 102)"><strong>DAILY LIST - BOOKINGS & REQUESTS</strong></div>

    <!--Line, yellow colored-->
    <div style="width: 325px; height: 5px; border-bottom: 4px solid rgb(255, 233, 26); position: absolute"></div>


    <br />
    <br />

    <table style="width: 100%; height: 122px; border: 2px solid white">
        <thead>
            <tr style="background-color:rgb(0, 102, 102)">
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.BookingId)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong> @Html.DisplayNameFor(model => model.Category)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.StartDate)</strong>
                    (for)
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.EndDate)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.TimeFrom)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.TimeTo)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.BookingDetail)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.Status)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.User.UserId)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.User.FirstName)</strong>
                </th>
                <th style="color:white; border: 2px solid white">
                    <strong>@Html.DisplayNameFor(model => model.User.LastName)</strong>
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr style="background-color: rgb(229,234,238)">
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.BookingId)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.Category)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.StartDate)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.EndDate)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.TimeFrom)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.TimeTo)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.BookingDetail)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.User.UserId)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.User.FirstName)
                </td>
                <td style="border: 2px solid white">
                    @Html.DisplayFor(modelItem => item.User.LastName)
                </td>
            </tr>
}
        </tbody>
    </table>





